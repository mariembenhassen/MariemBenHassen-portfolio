{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "DB_HOST": "localhost",
    "DB_USER": "root",
    "DB_PASSWORD": "",
    "DB_NAME": "contacts"
  }
}
